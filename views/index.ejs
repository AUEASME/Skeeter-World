    <main>
      <h1>Skeeter World ðŸ¦Ÿ</h1>
      <h2>
        Population dynamics simulation for mosquitoes infected with
        <em>Wolbachia</em> bacteria.
      </h2>
      <canvas id="world" style="display: none"></canvas>
      <div id="key" style="display: none">
        <p style="color: red">Uninfected</p>
        <p style="color: blue">Infected</p>
      </div>
      <form id="start__params">
        <!-- World Parameters -->
        <label for="infected__males"
          >Number of males to infect at outset (default 16):</label
        >
        <input type="number" id="infected__males" name="infected__males" />
        <label for="infected__females"
          >Number of females to infect at outset (default 32):</label
        >
        <input type="number" id="infected__females" name="infected__females" />
        <!-- Wolbachia Parameters -->
        <label for="toxin__length">
          Toxin/antitoxin length (default 4 bits):
        </label>
        <input type="number" id="toxin__length" name="toxin__length" />
        <label for="kill__rate"
          >Initial CI rate for <em>Wolbachia</em> (0 to 1, .5 by
          default):</label
        >
        <input type="number" id="kill__rate" name="kill__rate" />
        <label for="rescue__rate"
          >Initial CI rescue rate for <em>Wolbachia</em> (0 to 1, 1 by
          default):</label
        >
        <input type="number" id="rescue__rate" name="rescue__rate" />
        <!-- Simulation Parameters -->
        <label for="duration">Duration of simulation (default 365 days):</label>
        <input type="number" id="duration" name="duration" />
        <!-- Start -->
        <button id="start" onclick="startSimulation(event)">
          Start Simulation
        </button>
      </form>
      <button id="stop" onclick="stopSimulation(true)" style="display: none">
        Stop Simulation
      </button>
      <div id="plot" style="display: none"></div>
      <div id="toxin__plot" style="display: none"></div>
      <div id="antitoxin__plot" style="display: none"></div>
      <div id="mock__console" style="display: none"></div>
    </main>
    <script src="/js/main.js"></script>
    <script src="/js/plotly-2.34.0.min.js" charset="utf-8"></script>
