    <main>
      <h1>Skeeter World ðŸ¦Ÿ</h1>
      <h2>
        Population dynamics simulation for mosquitoes infected with
        <em>Wolbachia</em> bacteria.
      </h2>
      <div id="maps">
        <div class="map">
          <canvas id="world" style="display: none"></canvas>
          <div class="key" style="display: none">
            <p style="color: red">Uninfected</p>
            <p style="color: blue">Infected</p>
          </div>
        </div>
        <div class="map">
          <canvas id="water" style="display: none"></canvas>
          <div class="key" style="display: none">
            <p style="color: cornflowerblue;">Water</p>
            <p style="color: forestgreen">Dry Land</p>
          </div>
        </div>
      </div>
      <form id="start__params">
        <!-- World Parameters -->
        <h3>World Initialization Parameters</h3>
        <label for="infected__males"
          >% of males to infect at outset (0.0 â€” 1.0, default 0.25):</label
        >
        <input type="text" id="infected__males" name="infected__males" />
        <label for="infected__females"
          >% of females to infect at outset (0.0 â€” 1.0, default 0.25):</label
        >
        <input type="text" id="infected__females" name="infected__females" />
        <label for="water__percent"
          >% of world to be water (0.0 â€” 1.0, default 0.125):</label>
        <input type="text" id="water__percent" name="water__percent" />
        <!-- Wolbachia Parameters -->
        <h3>Wolbachia Parameters</h3>
        <label for="kill__rate"
          >Initial CI (kill) rate for <em>Wolbachia</em> (0.0 â€” 1.0, default 1.0):</label
        >
        <input type="text" id="kill__rate" name="kill__rate" />
        <label for="rescue__rate"
          >Initial rescue rate for <em>Wolbachia</em> (0.0 â€” 1.0, default 1.0):</label
        >
        <input type="text" id="rescue__rate" name="rescue__rate" />
        <label for="maximum__fitness__detriment"
          >Max. fitness detriment for <em>Wolbachia</em> (-1.0 â€” 0.0, default -1.0):</label
        >
        <input type="text" id="maximum__fitness__detriment" name="maximum__fitness__detriment" />
        <label for="maximum__fitness__benefit"
          >Max. fitness benefit for <em>Wolbachia</em> (0.0 â€” 1.0, default 1.0):</label
        >
        <input type="text" id="maximum__fitness__benefit" name="maximum__fitness__benefit" />
        <label for="minimum__infection__density"
          >Min. infection density for <em>Wolbachia</em> (0.0 â€” 1.0, default 0.5):</label
        >
        <input type="text" id="minimum__infection__density" name="minimum__infection__density" />
        <label for="maximum__infection__density"
          >Max. infection density for <em>Wolbachia</em> (0.0 â€” 1.0, default 1.0):</label
        >
        <input type="text" id="maximum__infection__density" name="maximum__infection__density" />
        <!-- Simulation Parameters -->
        <h3>Simulation Parameters</h3>
        <label for="repeats"># of times to run each unique simulation (default 1):</label>
        <input type="number" id="repeats" name="repeats" />
        <!-- Start -->
        <button id="start" onclick="startExperiment(event)">
          Start Simulation
        </button>
      </form>
      <div id="plot" style="display: none"></div>
      <div id="reproductive_success_plot" style="display: none"></div>
      <div id="mock__console" style="display: none"></div>
    </main>
    <script src="/js/main.js"></script>
    <script src="/js/plotly-2.34.0.min.js" charset="utf-8"></script>
  </body>
