    <main>
        <h1>Skeeter World ðŸ¦Ÿ</h1>
        <h2>Population dynamics simulation for mosquitoes infected with <em>Wolbachia</em> bacteria.</h2>
        <canvas id="world" style="display: none;"></canvas>
        <div id="key" style="display: none;">
            <p style="color: red;">Uninfected</p>
            <p style="color: #ff00ff;">Infected</p>
        </div>
        <form id="start__params">
            <!-- World Parameters -->
            <label for="capacity">Max cell capacity (default 64):</label>
            <input type="number" id="capacity" name="capacity">
            <p>Note: increasing the above value beyond the default may result in a slow simulation!</p>
            <label for="infected__males">Number of males to infect at outset (default 0):</label>
            <input type="number" id="infected__males" name="infected__males">
            <label for="infected__females">Number of females to infect at outset (default 5):</label>
            <input type="number" id="infected__females" name="infected__females">
            <!-- Wolbachia Parameters -->
            <label for="kill__rate">Initial CI rate for <em>Wolbachia</em> (0 to 1, random by default):</label>
            <input type="number" id="kill__rate" name="kill__rate">
            <label for="rescue__rate">Initial CI rescue rate for <em>Wolbachia</em> (0 to 1, random by default):</label>
            <input type="number" id="rescue__rate" name="rescue__rate">
            <label for="symbiote__rate">Start fitness impact for <em>Wolbachia</em> (-1 to 1, random by default):</label>
            <input type="number" id="symbiote__rate" name="symbiote__rate">
            <label for="mutation__rate">Self-mutation rate for <em>Wolbachia</em> (0 to 1, defining the percent chance of a mutation each generation, random by default):</label>
            <input type="number" id="mutation__rate" name="mutation__rate">
            <!-- Start -->
            <button id="start" onclick="startSimulation(event)">Start Simulation</button>
        </form>
        <div id="mock__console" style="display: none;">
            
        </div>
        <div id="plot" style="display: none;">

        </div>
    </main>
    <script src="/js/main.js"></script>
